<!DOCTYPE html>
<html>
    <head>
       <title>{% block title %}Alex Ortigosa Web{% endblock %}</title>



       {% block customcss %}
           <link href="https://fonts.googleapis.com/css?family=Oranienbaum" rel="stylesheet">
           <link rel="stylesheet" href="{{ asset('assets/normalize.css') }}"/>

           <link rel="stylesheet" href="{{ asset('assets/boots/css/bootstrap.css') }}"/>
           <link rel="stylesheet" href="{{ asset('assets/boots/css/bootstrap-theme.css') }}"/>
           <link rel="stylesheet" href="{{ asset('assets/global.css') }}"/>
       {% endblock %}
